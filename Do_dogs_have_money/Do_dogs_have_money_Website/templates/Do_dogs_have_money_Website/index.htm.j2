                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Accounts"><i class="ik ik-users"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Sales"><i class="ik ik-shopping-cart"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Purchase"><i class="ik ik-briefcase"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Pages"><i class="ik ik-clipboard"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Chats"><i class="ik ik-message-square"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Contacts"><i class="ik ik-map-pin"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Blocks"><i class="ik ik-inbox"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Events"><i class="ik ik-calendar"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="Notifications"><i class="ik ik-bell"></i></a>
                                    <a class="dropdown-item" href="#" data-toggle="tooltip" data-placement="top" title="More"><i class="ik ik-more-horizontal"></i></a>
                                </div>
                            </div>
                            <button type="button" class="nav-link ml-10" id="apps_modal_btn" data-toggle="modal" data-target="#appsModal"><i class="ik ik-grid"></i></button>
                            <div class="dropdown">
                                <a class="dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><img class="avatar" src="{% static 'Do_dogs_have_money_Website/img/user.jpg' %}" alt=""></a>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                                    <a class="dropdown-item" href="profile.html"><i class="ik ik-user dropdown-icon"></i> Profile</a>
                                    <a class="dropdown-item" href="#"><i class="ik ik-settings dropdown-icon"></i> Settings</a>
                                    <a class="dropdown-item" href="#"><span class="float-right"><span class="badge badge-primary">6</span></span><i class="ik ik-mail dropdown-icon"></i> Inbox</a>
                                    <a class="dropdown-item" href="#"><i class="ik ik-navigation dropdown-icon"></i> Message</a>
                                    <a class="dropdown-item" href="login.html"><i class="ik ik-power dropdown-icon"></i> Logout</a>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </header>
            <div class="page-wrap">
                <div class="app-sidebar colored">
                    <div class="sidebar-header">
                        <a class="header-brand" href="index.html">
                            <div class="logo-img">
                               <img src="{% static 'Do_dogs_have_money_Website/src/img/brand-white.svg' %}" class="header-brand-img" alt="lavalite"> 
                            </div>
                            <span class="text">DDHM</span>
                        </a>
                        <button type="button" class="nav-toggle"><i data-toggle="expanded" class="ik ik-toggle-right toggle-icon"></i></button>
                        <button id="sidebarClose" class="nav-close"><i class="ik ik-x"></i></button>
                    </div>
                    
                    <div class="sidebar-content">
                        <div class="nav-container">
                            <nav id="main-menu-navigation" class="navigation-main">
                                <div class="nav-item">
                                    <a href="newtransaction"><i class="ik ik-plus-square"></i><span>Nova Transação</span></a>
                                </div>
                                <div class="nav-lavel">Navegação</div>
                                <div class="nav-item">
                                    <a href="index.html"><i class="ik ik-bar-chart-2"></i><span>Visão Geral</span></a>
                                </div>
                                <div class="nav-item">
                                    <a href="pages/navbar.html"><i class="ik ik-menu"></i><span>Transações</span></a>
                                </div>
                                <div class="nav-item has-sub">
                                    <a href="javascript:void(0)"><i class="ik ik-chevrons-up"></i><span>Investimentos</span></a>
                                    <div class="submenu-content" style="">
                                        <a href="javascript:void(0)" class="menu-item">Adcionar Investimento</a>
                                        <a href="javascript:void(0)" class="menu-item">Gerenciar Investimentos</a>
                                    </div>
                                </div>
                                <div class="nav-item has-sub">
                                    <a href="javascript:void(0)"><i class="ik ik-chevrons-down"></i><span>Passivos</span></a>
                                    <div class="submenu-content" style="">
                                        <a href="javascript:void(0)" class="menu-item">Adcionar Passivo</a>
                                        <a href="javascript:void(0)" class="menu-item">Gerenciar Passivos</a>
                                    </div>
                                </div>
                                <div class="nav-item has-sub">
                                    <a href="javascript:void(0)"><i class="ik ik-settings"></i><span>Configuração</span></a>
                                    <div class="submenu-content" style="">
                                        <div class="nav-item has-sub">
                                            <a href="javascript:void(0)" class="menu-item">Contas</a>
                                            <div class="submenu-content" style="">
                                                <a href="javascript:void(0)" class="menu-item">Adcionar Contas</a>
                                                <a href="javascript:void(0)" class="menu-item">Editar Contas</a>
                                            </div>
                                        </div>
                                        <div class="nav-item has-sub">
                                            <a href="javascript:void(1)" class="menu-item">Categorias</a>
                                            <div class="submenu-content" style="">
                                                <a href="javascript:void(0)" class="menu-item">Adcionar Categorias</a>
                                                <a href="javascript:void(0)" class="menu-item">Editar Categorias</a>
                                            </div>
                                        </div>
                                            <a href="javascript:void(1)" class="menu-item">Configurações</a>
                                    </div>
                                </div>
                                <div class="nav-lavel">Contas</div>
        
                                {% for Account in Accounts%}
                                <div class="nav-item">
                                        <a href="#">
                                            <span>
                                                <div class="container-fluid"> 
                                                    <div class="row clearfix">
                                                        <div class="col-md-6 text-left" >{{ Account.Name }}</div>
                                                        <div class="col-md-6 text-right" {% if Account.BalanceSigned == "-" %} style="color:red" {% endif %}>{{ Account.BalanceSigned }}R${{ Account.Balance }}</div>
                                                    </div>
                                                </div>
                                            </span>
                                        </a>
                                    </div>
                                {% endfor %}
                                </nav>
                            </div>
                        </div>
                    </div>    
            

            <div class="main-content">
                <div class="container-fluid"> 
                    <div class="row clearfix">  
                        <div class="col-xl-3 col-md-6">
                            <div class="card prod-p-card card-red">
                                <div class="card-body" style="background-color:{% if GeneralInformation.BalanceSigned != '-' %}#26C281{% endif %}">
                                    <div class="row align-items-center mb-30">
                                        <div class="col">
                                            <h6 class="mb-5 text-white">Saldo</h6>
                                            <h3 class="mb-0 fw-700 text-white">{{ GeneralInformation.BalanceSigned }}R${{ GeneralInformation.Balance }}</h3>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fa fa-money-bill-alt text-{% if GeneralInformation.BalanceSigned != '-' %}green {% else %}red{% endif %} f-18"></i>
                                        </div>
                                    </div>
                                        <p class="mb-0 text-white"><span class="label label-danger mr-10">{{ GeneralInformation.BalancePercenteInformation }}%</span>{{ GeneralInformation.BalanceInformation }}</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-3 col-md-6">
                            <div class="card prod-p-card card-red">
                                <div class="card-body" style="background-color:{% if GeneralInformation.ExpensesSignedPercenteInformation == '-' %}#26C281{% endif %}">
                                    <div class="row align-items-center mb-30">
                                        <div class="col">
                                            <h6 class="mb-5 text-white">Saidas</h6>
                                            <h3 class="mb-0 fw-700 text-white">-R${{ GeneralInformation.Expenses }}</h3>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-money-check-alt text-{% if GeneralInformation.ExpensesSignedPercenteInformation == '-' %}green {% else %}red{% endif %} f-18"></i>
                                        </div>
                                    </div>
                                        <p class="mb-0 text-white"><span class="label label-danger mr-10">{{ GeneralInformation.ExpensesSignedPercenteInformation }}{{ GeneralInformation.ExpensesPercenteInformation }}%</span>{{ GeneralInformation.ExpensesInformation }}</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-3 col-md-6">
                            <div class="card prod-p-card card-red">
                                <div class="card-body" style="background-color:{% if GeneralInformation.DeltaActiveLiabilities == 1 %}#26C281{% endif %}">
                                    <div class="row align-items-center mb-30">
                                        <div class="col">
                                            <h6 class="mb-5 text-white">Ativos</h6>
                                            <h3 class="mb-0 fw-700 text-white">R${{ GeneralInformation.Active }}</h3>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fa fa-angle-double-up text-{% if GeneralInformation.DeltaActiveLiabilities == 1 %}green {% else %}red{% endif %} f-18"></i>
                                        </div>
                                    </div>
                                        <p class="mb-0 text-white"><span class="label label-danger mr-10">{{ GeneralInformation.ActiveSignedPercenteInformation }}{{ GeneralInformation.ActivePercenteInformation }}%</span>{{ GeneralInformation.ActiveInformation }}</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-3 col-md-6">
                            <div class="card prod-p-card card-red">
                                <div class="card-body" style="background-color:{% if GeneralInformation.DeltaActiveLiabilities == 1 %}#26C281{% endif %}">
                                    <div class="row align-items-center mb-30">
                                        <div class="col">
                                            <h6 class="mb-5 text-white">Passivos</h6>
                                            <h3 class="mb-0 fw-700 text-white">-R${{ GeneralInformation.Liabilities }}</h3>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fa fa-angle-double-down text-{% if GeneralInformation.DeltaActiveLiabilities == 1 %}green {% else %}red{% endif %} f-18"></i>
                                        </div>
                                    </div>
                                        <p class="mb-0 text-white"><span class="label label-danger mr-10">{{ GeneralInformation.LiabilitiesSignedPercenteInformation }}{{ GeneralInformation.LiabilitiesPercenteInformation }}%</span>{{ GeneralInformation.LiabilitiesInformation }}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        <script src="{% static 'Do_dogs_have_money_Website/plugins/moment/moment.js' %}"></script>
        <script src="{% static 'Do_dogs_have_money_Website/plugins/tempusdominus-bootstrap-4/build/js/tempusdominus-bootstrap-4.min.js' %}"></script>
        <script src="{% static 'Do_dogs_have_money_Website/plugins/d3/dist/d3.min.js' %}"></script>
        <script src="{% static 'Do_dogs_have_money_Website/plugins/c3/c3.min.js' %}"></script>
        <script src="{% static 'Do_dogs_have_money_Website/js/tables.js' %}"></script>
        <script src="{% static 'Do_dogs_have_money_Website/js/widgets.js' %}"></script>
